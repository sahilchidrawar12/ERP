//@each $i in ('50',' 80', '100', '120')
  .bg-size-#{$i}
    background-size: #{$i}px #{$i}px
.browser-list
  max-width: 400px
  margin: 0px auto

  a
    margin-right: 20px
    display: inline-block

.browser
  background-image: url('<%= asset_path 'browser_logos.png' %>')
  width: 90px
  height: 92px
  display: inline-block

.chrome
  background-position: -13px -6px
.firefox
  background-position: -121px -6px
.ie
  background-position: -435px -6px
